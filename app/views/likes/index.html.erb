<div class="page-header">
  <h1>
    Likes

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
            <th>Fabric</th>
        <th>Actions</th>
      </tr>

      <% @likes.each do |like| %>
      <tr>
        <td><img src="<%= like.fabric.fabric_image_url %>" style = "width:100px;height:100px;"</td>
        <td>

          <a href="/delete_like/<%= like.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
